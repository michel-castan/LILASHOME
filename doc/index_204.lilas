<?xml version="1.0" encoding="UTF-8"?>
<module>
  <doc>Square Roots by Newton's Method, conditional configuration</doc>
  <signal mode="constant" name="version" type="java.lang.Integer"/>
  <signal mode="in" name="x" sensitive="true" type="java.lang.Double" init="0.0"/>
  <signal mode="out" name="y" type="java.lang.Double" init="0.0"/>

  <case>
  	<when cond="version==1">
  		<Sqrt1 x="x" y="y"/>
  	</when>
  	<otherwise>
  		<Sqrt2 x="x" y="y"/>
  	</otherwise>
  </case>
  
</module>
